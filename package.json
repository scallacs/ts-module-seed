{
  "name": "ts-module-seed",
  "version": "0.0.1",
  "description": "Seed repo for new node modules using Typescript",
  "main": "dist/index",
  "typings": "dist/index",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "prepublish": "npm run build",
    "test": "./node_modules/.bin/mocha -r ts-node/register --recursive --reporter spec lib/**/*.spec.ts",
    "build": "./node_modules/.bin/rimraf ./dist & tsc",
    "lint": "tslint \"lib/**/*.ts\""
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/ivpusic/node-module-seed.git"
  },
  "keywords": [
    "seed",
    "node",
    "repo",
    "new",
    "projects"
  ],
  "author": "Stephane Leonard",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/scallacs/ts-module-seed/issues"
  },
  "devDependencies": {
    "@types/chai": "^4.0.1",
    "@types/mocha": "^2.2.41",
    "chai": "^4.1.0",
    "chai-as-promised": "^7.1.1",
    "mocha": "^3.2.0",
    "rimraf": "^2.6.1",
    "ts-node": "^3.3.0",
    "tslint": "^5.5.0",
    "typescript": "^2.4.2"
  }
}
